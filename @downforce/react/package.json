{
    "name": "@downforce/react",
    "version": "1.0.0",
    "type": "module",
    "sideEffects": false,
    "files": [
        "/LICENSE.txt",
        "/esm/",
        "/ts/"
    ],
    "exports": {
        "./*": "./esm/*.js"
    },
    "scripts": {
        "clean": "rm -rf ./esm/ || true; rm -rf ./node_modules/.cache/ || true",
        "build": "tsc --listEmittedFiles",
        "prepack": "npm run clean; npm run build",
        "preversion": "npm run build"
    },
    "dependencies": {
        "@downforce/std": "1",
        "@downforce/web": "1",
        "@types/react": "19",
        "@types/react-dom": "19",
        "tslib": "2"
    },
    "peerDependencies": {
        "react": "19",
        "react-dom": "19"
    },
    "devDependencies": {
        "typescript": "5.8"
    },
    "author": "Daniele Orlando",
    "license": "BSD-2-Clause",
    "description": "Awesome React",
    "keywords": [
        "React",
        "Hooks",
        "TypeScript",
        "CSS"
    ],
    "homepage": "https://github.com/downforce-tech/downforce.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/downforce-tech/downforce.js.git"
    },
    "bugs": {
        "url": "https://github.com/downforce-tech/downforce.js/issues"
    }
}
