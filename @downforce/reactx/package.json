{
    "name": "@downforce/reactx",
    "version": "1.0.0",
    "type": "module",
    "sideEffects": false,
    "files": [
        "/LICENSE.txt",
        "/**/*.css",
        "/**/*.js",
        "/**/*.d.ts"
    ],
    "exports": {
        "./*": "./*"
    },
    "scripts": {
        "clean": "rm -rf ./node_modules/.cache/ || true",
        "build": "tsc --listEmittedFiles",
        "prepack": "npm run clean; npm run build",
        "preversion": "npm run build"
    },
    "dependencies": {
        "@downforce/react": "1",
        "@downforce/std": "1",
        "@downforce/web": "1",
        "@downforce/webx": "1",
        "@types/react": "19",
        "@types/react-dom": "19",
        "tslib": "2"
    },
    "peerDependencies": {
        "@downforce/std.css": "1",
        "react": "19",
        "react-dom": "19"
    },
    "devDependencies": {
        "typescript": "5.8"
    },
    "author": "Daniele Orlando",
    "license": "BSD-2-Clause",
    "description": "Awesome React UI Widgets",
    "keywords": [
        "React",
        "UI",
        "Widgets",
        "TypeScript"
    ],
    "homepage": "https://github.com/downforce-tech/downforce.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/downforce-tech/downforce.js.git"
    },
    "bugs": {
        "url": "https://github.com/downforce-tech/downforce.js/issues"
    }
}
